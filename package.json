{
  "name": "wjhm-nextjs",
  "version": "1.2.0",
  "scripts": {
    "build": "next build && npm run vercel:sitemap",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "d": "yarn codegen:develop",
    "dev": "yarn codegen:develop",
    "develop": "run-p next:develop",
    "deploy": "yarn && yarn codegen && yarn codegen:fix && vercel:sitemap && yarn build && yarn export",
    "export": "next export",
    "format": "yarn prettier",
    "format-check": "yarn prettier --list-different",
    "icons": "svgr --no-dimensions --ext tsx -d src/components/atoms/icons src/assets/images/icons",
    "prettier": "prettier \"**/*.{css,js,json,jsx,md,mdx,scss,ts,tsx}\"",
    "lint": "yarn linter",
    "lint-fix": "yarn linter --fix",
    "linter": "eslint . --color",
    "serve": "next start -p 4000",
    "start": "yarn develop",
    "storybook": "yarn storybook:start",
    "test": "start-server-and-test dev 3000 cypress:dev",
    "test-cli": "start-server-and-test dev 3000 cypress:run",
    "validate": "yarn format-check && yarn lint",
    "validate-fix": "yarn format && yarn lint-fix && yarn validate",
    "upgrade": "npm-check --update",
    "codegen:develop": "run-s codegen codegen:fix vercel:sitemap develop",
    "codegen:fix": "node src/node/generation/fixSchema.ts",
    "cypress:dev": "cypress open --config baseUrl=http://localhost:3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "next:develop": "NODE_TLS_REJECT_UNAUTHORIZED=0 && NODE_OPTIONS=\"--max_old_space_size=8192\" next dev",
    "netlify:upload": "netlify deploy -p",
    "storybook:assets": "node src/node/storybook/assets.ts",
    "storybook:build": "build-storybook  -c src/.storybook -o storybook-public --quiet",
    "storybook:deploy": "run-s storybook:build storybook:assets netlify:upload",
    "storybook:start": "start-storybook -c src/.storybook -p 6006 -s public --quiet",
    "vercel:env-pull": "vercel env pull .env.local",
    "vercel:sitemap": "node src/node/generation/createSitemap.ts"
  },
  "dependencies": {
    "dayjs": "^1.10.4",
    "fetchql": "^3.0.0",
    "html-react-parser": "^1.2.4",
    "next": "^10.0.8",
    "next-pwa": "^5.0.5",
    "nprogress": "^0.2.0",
    "postmark": "^2.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-system": "^7.1.1",
    "react-intersection-observer": "^8.31.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-query": "^3.12.0",
    "react-twitter-embed": "^3.0.3",
    "react-youtube": "^7.13.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.11",
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-viewport": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.21",
    "@svgr/cli": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/cypress": "^7.0.4",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.1",
    "@types/react-youtube": "^7.6.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "graphql": "^15.5.0",
    "husky": "^5.1.3",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "react-query-devtools": "^2.6.3",
    "replace-in-file": "^6.2.0",
    "start-server-and-test": "^1.12.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.3"
  }
}
